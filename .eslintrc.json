{
  "extends": "airbnb",
  "plugins": [
    "react"
  ],
  "env" : {
    "jest" : true
  },
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 7,
    "ecmaFeatures": {
      "experimentalObjectRestSpread": true
    }
  },
  "rules": {
    "comma-dangle": [0, "never"],
    "no-console": 0,
    "import/no-unresolved": 0,
    "class-methods-use-this": 0,
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }]
  }
}
